class HTML():
    def __init__(self, cls_body):
        self.child = cls_body

    def __str__(self):
        return "<html>" + "\n" + self.child.__str__() + "\n" + "</html>"
class body(HTML):
    def __init__(self, cls_p, text):
        self.child = cls_p
        self.text = text
    def __str__(self):
        return "<body>" + "\n" + self.text + "\n" + self.child.__str__() + "\n" + "</body>"